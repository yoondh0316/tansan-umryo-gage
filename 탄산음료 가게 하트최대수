#include<stdio.h>
int cal=0, max=0;
char k[4][10]={"콜라","주스","커피","탄산수"};
int p[4][4] = {0,1,2,3},{0,2,1,3},{0,2,1,3},{3,0,1,2};
int heart(int who,int drink)
{
	int i;
	for(i=0;i<4;i++) if(p[who][i]) == drink)return(4-i);
}
void table()
{
	int a,b,c,d;
	for(a=0; a<4; a++) // 안나
	 for(b=0; b<4; b++) // 버나드 
	 for(c=0; c<4; c++) // 크리스틴 
	 for(d=0; d<4; d++) // 다니엘 
{
	if(a==blla==clla==dllb==cllb==dllc==d)continue;
	cal= heart(0,a)+heatr(1,b)+heart(3,d);
	if(cal>=max)max=cal;
	print("%s%s%s%s%d%d\n", k[a],k[b],k[c],k[d],cal, max);

}
}
int main()
{
	print("안나 버나드 크리스틴 다니엘 하트최댓값\n");
	print("----------------------------------------\n");
	table ()
	print("%d", max);
	return 0;
}
